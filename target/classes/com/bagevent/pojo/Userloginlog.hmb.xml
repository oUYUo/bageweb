<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.bagevent.pojo">
    <class name="Userloginlog" table="userloginlog">
        <id name="login_id" column="login_id">
            <generator class="native">
            </generator>
        </id>
        <property name="login_ip" column="login_ip"  />
        <property name="user_id" column="user_id"/>
        <property name="login_time" column="login_time"/>
    </class>

</hibernate-mapping>